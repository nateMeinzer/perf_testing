          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import java.text.SimpleDateFormat

// Generate dynamic run ID
if (vars.get(&quot;runID&quot;) == null) {
    def ts = new SimpleDateFormat(&quot;yyyyMMdd-HHmmss&quot;).format(new Date())
    vars.put(&quot;runID&quot;, &quot;run-noref-&quot; + ts)
}

def basePath = &quot;queries&quot;
def queryFile = vars.get(&quot;query_file&quot;)

log.info(&quot;üîç Starting PreProcessor for query_file = &apos;${queryFile}&apos;&quot;)

if (!queryFile) {
    log.error(&quot;‚ùå query_file is null or empty ‚Äî CSV may not be advancing.&quot;)
    throw new IllegalStateException(&quot;query_file is missing.&quot;)
}

// Normalize path: avoid double slashes
def fullPath = new File(basePath, queryFile).getCanonicalPath()
def file = new File(fullPath)

log.info(&quot;üìÑ Attempting to load file from: ${file.absolutePath}&quot;)

if (!file.exists()) {
    log.error(&quot;‚ùå File does not exist: ${file.absolutePath}&quot;)
    throw new FileNotFoundException(&quot;SQL file not found at: ${file.absolutePath}&quot;)
}

def query = file.getText(&quot;UTF-8&quot;)
vars.put(&quot;sql_query&quot;, query)

// Extract filename for tagging
def fileName = queryFile.tokenize(&apos;/&apos;).last()
vars.put(&quot;queryFileName&quot;, fileName)

log.info(&quot;‚úÖ Successfully loaded &apos;${queryFile}&apos; (${query.length()} chars)&quot;)
log.debug(&quot;üìù Query starts with: &quot; + query.take(80).replaceAll(&quot;\\s+&quot;, &quot; &quot;) + &quot;...&quot;)</stringProp>
        </JSR223PreProcessor>
